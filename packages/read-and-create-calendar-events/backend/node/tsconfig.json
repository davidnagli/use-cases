{
    "compilerOptions": {
      "target": "ESNext",
      "lib": ["ESNext"],
      "allowJs": true,
      "checkJs": true,
      "skipLibCheck": false,
      "noImplicitAny": false,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "module": "ESNext",
      "moduleResolution": "Node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "declaration": true,
    },
    "include": ["./"]
}